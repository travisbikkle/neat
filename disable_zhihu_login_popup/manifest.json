{
    "name": "edge helper",
    "version": "0.0.0.2",
    "manifest_version": 3,
    "description": "A chromium extension that optimizes your online experience.",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "options.html"
    },
    "content_scripts": [
      {
        "matches": [
          "https://*.zhihu.com/*"
        ],
        "js": [
          "js/zhihu.js"
        ],
        "run_at": "document_end"
      },
      {
        "matches": [
          "https://*.csdn.net/*"
        ],
        "js": [
          "js/csdn_copy.js"
        ],
        "run_at": "document_end"
      }
    ]
}
