{
    "name": "Neat 干净上网",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "还你一个不流氓的简中网络环境",
    "permissions": [
      "storage",
      "clipboardWrite"
    ],
    "action": {
      "default_popup": "options.html"
    },
    "content_scripts": [
      {
        "matches": [
          "https://*.zhihu.com/*"
        ],
        "js": [
          "js/zhihu.js"
        ],
        "run_at": "document_end"
      },
      {
        "matches": [
          "https://*.csdn.net/*",
          "https://*.qidian.com/*",
          "https://qidian.com/*"
        ],
        "js": [
          "js/copy.js"
        ],
        "run_at": "document_end"
      }
    ]
}
